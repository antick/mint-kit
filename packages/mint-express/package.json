{
  "name": "mint-express",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --parallel --target=serve --projects=mint-express,pumpkin-web --maxParallel=10",
    "start:docusaurus": "nx serve mint-docusaurus",
    "start:express": "nx serve mint-express",
    "start:nest": "nx serve mint-nest",
    "build": "nx build",
    "test": "nx test",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "docker": "docker-compose -f docker-compose.yml up",
    "lint": "eslint ./src/ --ignore-path .gitignore",
    "lint:fix": "pnpm run lint --fix",
    "lint:watch": "pnpm run lint --watch",
    "install:clean": "rm -rf node_modules/ && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "autoprefixer": "10.4.8",
    "bcryptjs": "2.4.3",
    "clsx": "^1.1.1",
    "compression": "1.7.4",
    "core-js": "3.24.1",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.18.1",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "5.5.1",
    "helmet": "4.6.0",
    "http-status": "1.5.2",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.5.4",
    "mongoose-paginate-v2": "1.7.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.7.8",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "validator": "13.7.0",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "coveralls": "3.1.1",
    "faker": "5.5.3",
    "node-mocks-http": "1.11.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
