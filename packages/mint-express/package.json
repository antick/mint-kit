{
  "name": "mint-express",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/antick/mint-kit.git",
  "homepage": "https://github.com/antick/mint-kit",
  "author": "Pankaj Sanam",
  "description": "A starter kit/boilerplate for all your full-stack apps",
  "keywords": [
    "mint",
    "mint express",
    "boilerplate",
    "mern",
    "node",
    "nodejs",
    "express",
    "api",
    "backend",
    "monorepo"
  ],
  "scripts": {
    "start": "nx run-many --parallel --target=serve --projects=mint-express,pumpkin-web --maxParallel=10",
    "start:docusaurus": "nx serve mint-docusaurus",
    "start:express": "nx serve mint-express",
    "start:nest": "nx serve mint-nest",
    "build": "nx build",
    "test": "nx test",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "docker": "docker-compose -f docker-compose.yml up",
    "lint": "eslint ./src/ --ignore-path .gitignore",
    "lint:fix": "pnpm run lint --fix",
    "lint:watch": "pnpm run lint --watch",
    "install:clean": "rm -rf node_modules/ && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "autoprefixer": "10.4.8",
    "bcryptjs": "2.4.3",
    "clsx": "^1.1.1",
    "compression": "1.7.4",
    "core-js": "3.24.1",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.18.1",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "5.5.1",
    "helmet": "4.6.0",
    "http-status": "1.5.2",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "mongoose": "6.5.4",
    "mongoose-paginate-v2": "1.7.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.7.8",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "postcss": "8.4.16",
    "reflect-metadata": "0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "7.5.6",
    "tslib": "2.4.0",
    "validator": "13.7.0",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@nrwl/node": "14.6.4",
    "@nrwl/workspace": "14.6.4",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.33.1",
    "babel-jest": "28.1.3",
    "coveralls": "3.1.1",
    "cypress": "10.3.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-security": "1.5.0",
    "faker": "5.5.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "lerna": "^6.0.1",
    "node-mocks-http": "1.11.0",
    "nx": "14.6.4",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
