{
  "name": "mint-react",
  "version": "0.1.0",
  "author": {
    "name": "Pankaj Sanam",
    "email": "pankaj@desk.sh",
    "url": "https://pankajsanam.github.io"
  },
  "keywords": [
    "mint",
    "mint react",
    "boilerplate",
    "mern",
    "react",
    "hooks",
    "frontend"
  ],
  "description": "A react + tailwind css boilerplate",
  "license": "MIT",
  "repository": "https://github.com/antick/mint-react.git",
  "scripts": {
    "watch:css": "postcss src/assets/styles.css -o public/style.css --watch --max_old_space_size=8192",
    "start": "cross-env PORT=3001 react-scripts start",
    "lint": "eslint ./src/ --ignore-path .gitignore",
    "test": "npm run lint && jest -i --coverage",
    "test:e2e": "cypress open",
    "test:watch": "jest --watchAll",
    "test:default": "react-scripts test",
    "lint:fix": "npm run lint --fix",
    "lint:watch": "npm run lint --watch",
    "build:css": "tailwind build src/assets/styles.css -o public/style.css",
    "build": "react-scripts build",
    "build:prod": "cross-env npm run build",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "docker": "docker-compose -f docker-compose.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "install:clean": "rm -rf node_modules/ && rm -f package-lock.json && npm install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@tailwindcss/forms": "0.3.4",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@vx/group": "0.0.199",
    "@vx/mock-data": "0.0.199",
    "@vx/scale": "0.0.199",
    "@vx/shape": "0.0.199",
    "apexcharts": "3.36.0",
    "axios": "0.23.0",
    "connected-react-router": "6.9.3",
    "cross-env": "7.0.3",
    "formik": "2.2.9",
    "framer": "1.3.6",
    "framer-motion": "4.1.17",
    "history": "4.10.1",
    "jest-environment-jsdom": "27.5.1",
    "js-cookie": "2.2.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-apexcharts": "1.4.0",
    "react-charts": "2.0.0-beta.7",
    "react-dom": "17.0.2",
    "react-feather": "2.0.10",
    "react-icons": "4.6.0",
    "react-images-upload": "1.2.8",
    "react-redux": "7.2.9",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-scripts": "4.0.3",
    "react-spinners": "0.11.0",
    "recharts": "2.1.15",
    "redux": "4.2.0",
    "redux-logger": "4.0.0",
    "redux-thunk": "2.4.1",
    "svg-jest": "1.0.1",
    "victory": "36.6.8",
    "viser-react": "2.4.8",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/plugin-transform-modules-commonjs": "7.18.6",
    "@babel/preset-env": "7.19.4",
    "@redux-devtools/core": "3.13.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "autoprefixer": "10.4.12",
    "babel-cli": "6.26.0",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.1.1",
    "cypress": "8.7.0",
    "cypress-plugin-snapshots": "1.4.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "25.7.0",
    "eslint-plugin-jest-dom": "3.9.4",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-testing-library": "4.12.4",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-css-modules-transform": "4.4.2",
    "jpeg-js": ">=0.4.0",
    "js-base64": "2.6.4",
