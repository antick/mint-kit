{
  "name": "mint-react",
  "version": "0.1.0",
  "keywords": [
    "mint",
    "mint react",
    "boilerplate",
    "mern",
    "react",
    "hooks",
    "frontend"
  ],
  "description": "A react + tailwind css boilerplate",
  "license": "MIT",
  "scripts": {
    "watch:css": "postcss src/assets/styles.css -o public/style.css --watch --max_old_space_size=8192",
    "start": "cross-env PORT=3001 react-scripts start",
    "lint": "eslint ./src/ --ignore-path .gitignore",
    "test": "npm run lint && jest -i --coverage",
    "test:e2e": "cypress open",
    "test:watch": "jest --watchAll",
    "test:default": "react-scripts test",
    "lint:fix": "npm run lint --fix",
    "lint:watch": "npm run lint --watch",
    "build:css": "tailwind build src/assets/styles.css -o public/style.css",
    "build": "react-scripts build",
    "build:prod": "cross-env npm run build",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "docker": "docker-compose -f docker-compose.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "install:clean": "rm -rf node_modules/ && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.6",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@vx/group": "0.0.199",
    "@vx/mock-data": "0.0.199",
    "@vx/scale": "0.0.199",
    "@vx/shape": "0.0.199",
    "apexcharts": "3.43.0",
    "axios": "1.5.1",
    "connected-react-router": "6.9.3",
    "cross-env": "7.0.3",
    "formik": "2.4.5",
    "framer": "2.4.1",
    "framer-motion": "10.16.4",
    "history": "4.10.1",
    "jest-environment-jsdom": "29.7.0",
    "js-cookie": "3.0.5",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-apexcharts": "1.4.1",
    "react-charts": "2.0.0-beta.7",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-icons": "4.11.0",
    "react-images-upload": "1.2.8",
    "react-redux": "8.1.3",
    "react-router": "6.16.0",
    "react-router-dom": "6.16.0",
    "react-scripts": "5.0.1",
    "react-spinners": "0.13.8",
    "recharts": "2.8.0",
    "redux": "4.2.1",
    "redux-logger": "4.0.0",
    "redux-thunk": "2.4.2",
    "svg-jest": "1.0.1",
    "victory": "36.6.11",
    "viser-react": "2.4.8",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/plugin-transform-modules-commonjs": "7.23.0",
    "@babel/preset-env": "7.22.20",
    "@redux-devtools/core": "3.13.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
    "autoprefixer": "10.4.16",
    "babel-cli": "6.26.0",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.1.1",
    "cypress": "13.3.0",
    "cypress-plugin-snapshots": "1.4.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "enzyme-to-json": "3.6.2",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-testing-library": "6.0.2",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-css-modules-transform": "4.4.2",
    "jpeg-js": "0.4.4",
    "js-base64": "2.6.4",
    "merge": "2.1.1",
    "path": "0.12.7",
    "postcss": "8.4.31",
    "postcss-cli": "10.1.0",
    "postcss-import": "15.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "18.2.0",
    "redux-devtools-extension": "2.13.9",
    "tailwindcss": "3.3.3"
  }
}
