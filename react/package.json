{
  "name": "mint-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "dev": "pnpm run routes:watch && vite",
    "build": "pnpm run routes:generate && tsc -b && vite build",
    "lint": "eslint ./src/ --ignore-path .gitignore",
    "preview": "vite preview",
    "routes:generate": "tsr generate",
    "routes:watch": "tsr watch",
    "watch:css": "postcss src/assets/styles.css -o public/style.css --watch --max_old_space_size=8192",
    "lint:fix": "pnpm run lint --fix",
    "lint:watch": "pnpm run lint --watch",
    "build:css": "tailwind build src/assets/styles.css -o public/style.css",
    "docker": "docker-compose -f docker-compose.yml up",
    "install:clean": "rm -rf node_modules/ && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.7",
    "@tanstack/react-router": "1.46.8",
    "@vx/group": "0.0.199",
    "@vx/mock-data": "0.0.199",
    "@vx/scale": "0.0.199",
    "@vx/shape": "0.0.199",
    "apexcharts": "3.52.0",
    "axios": "1.8.2",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "formik": "2.4.6",
    "framer": "2.4.1",
    "framer-motion": "11.3.24",
    "joi": "17.13.3",
    "js-cookie": "3.0.5",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "react": "18.3.1",
    "react-apexcharts": "1.4.1",
    "react-charts": "2.1.0",
    "react-dom": "18.3.1",
    "react-feather": "2.0.10",
    "react-icons": "5.2.1",
    "react-redux": "9.1.2",
    "react-spinners": "0.14.1",
    "recharts": "2.12.7",
    "redux": "5.0.1",
    "redux-logger": "4.0.0",
    "redux-thunk": "3.1.0",
    "victory": "37.0.2",
    "viser-react": "2.4.8",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@redux-devtools/core": "4.0.0",
    "@redux-devtools/extension": "3.3.0",
    "@tailwindcss/forms": "0.5.7",
    "@tanstack/router-cli": "1.47.0",
    "@tanstack/router-devtools": "1.46.9",
    "@tanstack/router-plugin": "1.47.0",
    "@types/js-cookie": "3.0.6",
    "@types/lodash": "4.17.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/redux-logger": "3.0.13",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "@typescript-eslint/parser": "8.0.1",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.9",
    "eslint-plugin-security": "3.0.1",
    "merge": "2.1.1",
