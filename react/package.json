{
  "name": "mint-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "cross-env PORT=3001 vite",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint ./src/ --ignore-path .gitignore",
    "preview": "vite preview",
    "watch:css": "postcss src/assets/styles.css -o public/style.css --watch --max_old_space_size=8192",
    "lint:fix": "npm run lint --fix",
    "lint:watch": "npm run lint --watch",
    "build:css": "tailwind build src/assets/styles.css -o public/style.css",
    "build:prod": "cross-env npm run build",
    "docker": "docker-compose -f docker-compose.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "install:clean": "rm -rf node_modules/ && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.7",
    "@vx/group": "0.0.199",
    "@vx/mock-data": "0.0.199",
    "@vx/scale": "0.0.199",
    "@vx/shape": "0.0.199",
    "apexcharts": "3.52.0",
    "axios": "1.7.3",
    "connected-react-router": "6.9.3",
    "cross-env": "7.0.3",
    "dotenv": "16.4.5",
    "formik": "2.4.6",
    "framer": "2.4.1",
    "framer-motion": "11.3.23",
    "history": "4.10.1",
    "joi": "17.13.3",
    "js-cookie": "3.0.5",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "react": "18.3.1",
    "react-apexcharts": "1.4.1",
    "react-charts": "2.1.0",
    "react-dom": "18.3.1",
    "react-feather": "2.0.10",
    "react-icons": "5.2.1",
    "react-images-upload": "1.2.8",
    "react-redux": "9.1.2",
    "react-router": "6.26.0",
    "react-router-dom": "6.26.0",
    "react-spinners": "0.14.1",
    "recharts": "2.12.7",
    "redux": "5.0.1",
    "redux-logger": "4.0.0",
    "redux-thunk": "3.1.0",
    "victory": "37.0.2",
    "viser-react": "2.4.8",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@redux-devtools/core": "4.0.0",
    "@redux-devtools/extension": "3.3.0",
    "@tailwindcss/forms": "0.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.9",
    "eslint-plugin-security": "3.0.1",
    "merge": "2.1.1",
    "path": "0.12.7",
    "postcss": "8.4.41",
    "postcss-cli": "11.0.0",
    "postcss-import": "16.1.0",
    "prop-types": "15.8.1",
    "tailwindcss": "3.4.8",
    "vite": "5.4.0"
  },
  "overrides": {
    "eslint": "$eslint"
  }
}
